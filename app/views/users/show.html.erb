<h1><%= image_tag(@user.profile_image.icon.url) if @user.profile_image && @user.profile_image.icon.url %>  <%= @user.name %></h1>
<%= form_with(model: @user, url: users_path, local: true) do |f| %>
  <%= f.hidden_field :profile_image_cache %>
  <br>
  <br>
  <p>user email:<%= @user.email %></p>
<% end %>
<br>
<td><%= link_to 'My favorites', favorites_path if @user.id == current_user.id %></td>
<br>
<br>
<%= link_to 'Edit your profile', edit_user_path(@user.id) %>
